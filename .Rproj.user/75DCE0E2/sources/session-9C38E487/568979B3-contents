library(smacof)
library(ProDistance)
data("FoodData")
data("covFoodData")
out <- orthogonalDistanceMatrix(FoodData)

stress <-c()

for(i in 1:20){

  stress[i] <- mds(out,ndim = i,
                   type = "mspline")$stress

}

opt  = which(stress < 0.05)[1]

mds_R <- mds(out,type = "mspline", ndim = opt)

db <- db <- cbind(mds_R$conf, covFoodData)

save(db = db,
     file = "C:/Users/livio/Desktop/mds/Data/FoodData/Output/output_mds_R.RData")
